{
  "scripts": {
    "ts:w": "tsc -w",
    "ts": "tsc",
    "prestart": "rm -rf build && tsc",
    "start": "node --max-old-space-size=8192 build/index.js",
    "dev": "nodemon",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/helmet": "^0.0.44",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/sequelize": "^4.28.5",
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "module-alias": "^2.2.1",
    "mysql2": "^1.7.0",
    "sequelize": "^5.19.1",
    "source-map-support": "^0.5.13",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "verbose": true,
    "exec": "ts-node ./src/index.ts --project ./tsconfig.json",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "_moduleAliases": {
    "@classes": "build/Classes/index.js",
    "@utils": "build/Utils/index.js"
  }
}
